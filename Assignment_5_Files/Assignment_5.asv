close all; clear all; clc;
format compact;

%% Contrast Enhancement
PATH = {'imageCE1.tif','imageCE2.tif','imageCE3.tif','imageCE4.tif','imageCE1.tif',};
for i = 1:length(PATH)-4
    IMG = imread(PATH{i});
    figure(i);
    subplot(1,2,1);
    imshow(IMG);
    subplot(1,2,2);
    [Count, Bin] = imhist(IMG,10);
    
end

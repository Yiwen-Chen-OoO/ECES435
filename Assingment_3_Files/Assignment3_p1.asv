close all; clear all; clc;
format compact;
%the path of image file, will be used
PATH = {'peppers.tif','baboon.tif','LSBwmk1.tiff','LSBwmk2.tiff','LSBwmk3.tiff'};
for j = 1:length(PATH)
figure(j)
Image = imread(string(PATH(j)));
for i = 1:8
    Img_b = getPlane(Image,i);
    subplot(2,4,i);
    imshow(Img_b);
    title(['Bit Plane',num2str(i)]);
end
end


